"Name"    = 'ate-bc'
"Author"  = 'jhon james quintero'
"Version" = '0.1-dev'
"About"   = 'basic calculator grammar'

"Case Sensitive" = True 

"Start Symbol" = <Exp>
                  
IntLiteral = {Number}|{Number}+
                    
<Exp> ::= <Term> '+' <Exp>
            | <Term> '-' <Exp>
            | <Term>
          
<Term> ::= <Factor> '/' <Term>            
            | <Factor> '*' <Term>
            | <Factor>

<Factor> ::= IntLiteral
          | '-' IntLiteral 

